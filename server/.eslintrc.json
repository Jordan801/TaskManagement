{
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": [2,"never"],
    "no-cond-assign": [2,"always"],
    "no-console": 0,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2,"all"],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "accessor-pairs": [2,{"getWithoutSet":true,"setWithoutGet":true}],
    "block-scoped-var": 2,
    "complexity": 2,
    "consistent-return": 2,
    "curly": [2,"all"],
    "default-case": 2,
    "dot-notation": 2,
    "dot-location": [2,"property"],
    "eqeqeq": [2,"smart"],
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-empty-label": 0,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": [2,{"boolean":true,"number":true,"string":true}],
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-process-env": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": [2,"always"],
    "no-script-url": 0,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 0,
    "no-useless-call": 2,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-with": 2,
    "radix": 0,
    "vars-on-top": 2,
    "wrap-iife": 0,
    "yoda": [2,"never",{}],
    "strict": [2,"global"],
    "init-declarations": 0,
    "no-catch-shadow": 0,
    "no-delete-var": 2,
    "no-label-var": 0,
    "no-shadow-restricted-names": 0,
    "no-shadow": 0,
    "no-undef-init": 0,
    "no-undef": 2,
    "no-undefined": 0,
    "no-unused-vars": 2,
    "no-use-before-define": 0,
    "callback-return": 2,
    "handle-callback-err": 2,
    "no-mixed-requires": [2,false],
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-exit": 2,
    "no-restricted-modules": 0,
    "no-sync": 0,
    "array-bracket-spacing": [2,"always",{"singleValue":true,"objectsInArrays":true,"arraysInArrays":true}],
    "brace-style": [2,"1tbs",{}],
    "camelcase": 0,
    "comma-spacing": [2,{"after":true}],
    "comma-style": 2,
    "computed-property-spacing": [2,"always"],
    "consistent-this": 0,
    "eol-last": 0,
    "func-names": 0,
    "func-style": [2,"declaration"],
    "id-length": [2,{"min":2,"max":32,"exceptions":["[\"i\"]"]}],
    "indent": [2,4],
    "key-spacing": [2,{"align":"value","afterColon":true}],
    "lines-around-comment": [2,{"beforeBlockComment":true,"afterBlockComment":true}],
    "linebreak-style": 0,
    "max-nested-callbacks": 2,
    "new-cap": [2,{"newIsCap":true}],
    "new-parens": 2,
    "newline-after-var": [2,"always"],
    "no-array-constructor": 0,
    "no-continue": 0,
    "no-inline-comments": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2,{"max":2}],
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-ternary": 0,
    "no-trailing-spaces": [2,{"skipBlankLines":true}],
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [2,"always",{"arraysInObjects":true,"objectsInObjects":true}],
    "one-var": 0,
    "operator-assignment": 0,
    "operator-linebreak": [2,"none"],
    "padded-blocks": [2,"always"],
    "quote-props": 0,
    "quotes": [2,"double","avoid-escape"],
    "id-match": 0,
    "semi-spacing": 0,
    "semi": [2,"always"],
    "sort-vars": 2,
    "keyword-spacing": 2,
    "space-before-blocks": [2,"always"],
    "space-before-function-paren": 2,
    "space-in-parens": 0,
    "space-infix-ops": 2,
    "space-unary-ops": [2,{"words":true,"nonwords":true}],
    "spaced-comment": 0,
    "wrap-regex": 0,
    "arrow-parens": 2,
    "arrow-spacing": [2,{"before":true,"after":true}],
    "constructor-super": 2,
    "generator-star-spacing": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": [2,"always"],
    "prefer-const": 2,
    "prefer-spread": 2,
    "prefer-reflect": 0,
    "require-yield": 2,
    "max-depth": 0,
    "max-len": 0,
    "max-params": 0,
    "max-statements": 0,
    "no-bitwise": 0,
    "no-plusplus": 0
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "globals": {
    "Promise": true
  }
}